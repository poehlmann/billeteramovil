<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <security>
            <requestFiltering removeServerHeader="true" />
        </security>
        <httpProtocol>
            <customHeaders>
                  <add name="X-Frame-Options" value="SAMEORIGIN" />
                  <add name="Strict-Transport-Security" value="max-age=31536000; includeSubDomains" />
                  <add name="X-XSS-Protection" value="1; mode=block" />
                  <add name="X-Content-Type-Options" value="nosniff" />
                  <add name="Content-Security-Policy" value="default-src 'none'; script-src connect.facebook.net www.googletagmanager.com 'sha256-Grip+tmkF9Fl1Vyq5WyNf/HiLlajxt0SLMs8NGMT7gU=' 'sha256-3k7fPCD1Omi36FAsdLKdQagMZo/e5a/dMju8h7ufQHc='; style-src 'none'; img-src 'none'; child-src td.doubleclick.net; font-src 'none'; connect-src 'self'; media-src 'none'; object-src 'none';" />
                  <add name="Cache-Control" value="public, max-age=86400" />
                  <add name="Referrer-Policy" value="no-referrer" />
                  <add name="Permissions-Policy" value="accelerometer=(),autoplay=(),fullscreen=*,geolocation=*,gyroscope=()" />
                  <remove name="X-Powered-By" />
            </customHeaders>
        </httpProtocol>
    </system.webServer>
</configuration>
